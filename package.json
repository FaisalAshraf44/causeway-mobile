{
  "name": "causeWay",
  "version": "0.0.1",
  "private": true,
  "reactNativePermissionsIOS": [
    "LocationAccuracy",
    "LocationAlways",
    "LocationWhenInUse"
  ],
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "clean:android": "cd android && ./gradlew cleanBuildCache && ./gradlew clean && cd ..",
    "setup:ios": "cd ios && pod install && cd ..",
    "clean:ios": "rm -rf ios/Pods && rm -rf ios/Podfile.lock",
    "clean": "rm -rf node_modules && rm -rf package-lock.json",
    "android:production-release": "ENVFILE=.env.production react-native run-android --variant=release",
    "android:staging-release": "ENVFILE=.env.staging  react-native run-android --variant=release"
  },
  "dependencies": {
    "@miblanchard/react-native-slider": "^2.3.1",
    "@react-native-async-storage/async-storage": "^1.17.6",
    "@react-native-community/blur": "^4.3.0",
    "@react-native-community/geolocation": "^3.0.6",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-native-firebase/app": "^14.11.0",
    "@react-native-firebase/database": "^14.11.0",
    "@react-native-firebase/firestore": "^14.11.0",
    "@react-native-firebase/storage": "^14.11.0",
    "@react-navigation/bottom-tabs": "^6.3.1",
    "@react-navigation/drawer": "^6.4.1",
    "@react-navigation/material-top-tabs": "^6.6.2",
    "@react-navigation/native": "^6.0.10",
    "@react-navigation/native-stack": "^6.6.2",
    "@reduxjs/toolkit": "^1.8.2",
    "@stripe/stripe-react-native": "^0.26",
    "appcenter": "4.4.4",
    "axios": "^0.27.2",
    "geolib": "^3.3.3",
    "i18next": "^21.8.5",
    "jetifier": "^2.0.0",
    "lottie-ios": "3.4.0",
    "lottie-react-native": "^5.1.5",
    "moment": "^2.29.4",
    "react": "18.1.0",
    "react-hook-form": "^7.43.9",
    "react-i18next": "^11.16.9",
    "react-native": "0.68.2",
    "react-native-bouncy-checkbox": "^3.0.7",
    "react-native-calendars": "^1.1295.0",
    "react-native-code-push": "^7.0.4",
    "react-native-config": "^1.4.5",
    "react-native-device-info": "^10.6.0",
    "react-native-element-dropdown": "^2.9.0",
    "react-native-error-boundary": "^1.2.3",
    "react-native-fast-image": "^8.6.3",
    "react-native-gesture-handler": "^2.9.0",
    "react-native-gifted-chat": "^2.1.0",
    "react-native-google-places-autocomplete": "^2.5.1",
    "react-native-image-picker": "5.4.2",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-maps": "^1.7.1",
    "react-native-otp-textinput": "^1.0.1",
    "react-native-pager-view": "^6.2.0",
    "react-native-paper": "^4.12.1",
    "react-native-permissions": "^3.8.0",
    "react-native-reanimated": "^2.8.0",
    "react-native-reanimated-carousel": "^3.3.2",
    "react-native-responsive-screen": "^1.4.2",
    "react-native-restart": "^0.0.24",
    "react-native-safe-area-context": "^4.2.5",
    "react-native-screens": "^3.13.1",
    "react-native-splash-screen": "^3.3.0",
    "react-native-svg": "^13.9.0",
    "react-native-tab-view": "^3.5.1",
    "react-native-vector-icons": "^8.1.0",
    "react-redux": "^7.2.5",
    "redux": "^4.2.0",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.3"
